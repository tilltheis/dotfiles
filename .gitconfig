	name = Till Theis
	email = theis.till@gmail.com
[push]
	default = simple
[core]
  excludesfile = ~/.gitignore
[color]
  ui = true
  diff = always
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
  whitespace = red reverse
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
